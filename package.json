    "express": "^4.21.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "mongoose": "^8.2.3",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.0",
    "otp-generator": "^4.0.1",
